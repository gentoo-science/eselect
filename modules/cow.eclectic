DESCRIPTION="Do things to a cow"
MAINTAINER="ciaranm@gentoo.org"
SVN_DATE='$Date$'
VERSION=$(svn_date_to_version "${SVN_DATE}" )

### moo action

describe_moo() {
	echo "Say moo"
}

do_moo() {
	echo "${@:-I am a cow}" | cowsay
}

### think action

describe_think() {
	echo "Show a pensive cow"
}

do_think() {
	echo "${@:-Am I a cow?}" | cowthink
}

